{
  "name": "naf-uws-adapter",
  "version": "0.1.1",
  "description": "networked-aframe uws network adapter",
  "main": "dist/naf-uws-adapter.js",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^5.1.0",
    "webpack": "^3.8.1",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "express": "^4.16.2",
    "uws": "^8.14.1"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production node ./server/index.js",
    "dev": "cross-env NODE_ENV=development node ./server/index.js",
    "build": "npm run build:dev && npm run build:prod",
    "build:dev": "webpack --config webpack.dev.js",
    "build:prod": "webpack --config webpack.prod.js -p",
    "prepublish": "npm run build"
  }
}
